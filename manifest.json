{
  "manifest_version": 2,
  "name": "Self-Care Twitter",
  "version": "1.0",
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": [
        "https://twitter.com/home",
        "https://www.twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": ["./jquery-3.1.1.min.js", "./main.js"],
      "css": ["./index.css"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
